em3\">\n      <h3>Price: {{ price }}</h3>\n      <h3>Quantity: {{ quantity }}</h3>\n    </div>\n\n    <!-- CART BUTTONS -->\n    <div class=\"grid-item item4\">\n      <Button\n        @click=\"addToCart()\"\n        id=\"add_to_cart_id\"\n        class=\"buttons\"\n        text=\"Add To Cart\"\n        color=\"green\"\n      />\n      <!-- @click=\"$store.commit('addToCart')\" -->\n      <br />\n      <Button\n        id=\"clear_cart_id\"\n        class=\"buttons\"\n        text=\"Clear Cart\"\n        color=\"red\"\n      />\n    </div>\n  </div>\n</template>\n\n<script>\nimport '../../public/style/style.scss'\nimport Button from \"../components/Button\"\n\n\nexport default {\n  name: \"Products\",\n  components: {\n    Button,\n  },\n  data() {\n    return {\n      id: 1,\n      title: \"Advanced Beard Growth Kit\",\n      price: \"$57.00\",\n      quantity: 1,\n      text: \"This is a great product. Cant wait for you to receive it\",\n      productImage: \"../assets/beard_club.png\",\n    };\n  },\n  methods: {\n    addToCart() {\n\n      var addItem = {\n        id: this.id,\n        title: this.title,\n        price: this.price,\n        quantity: this.quantity,\n      };\n      console.log(\"comp: Products | Add Item to Cart: \", addItem);\n      this.$emit(\"add-to-cart\", addItem);\n\n    },\n  },\n};\n</script>\n"]}]}